<% if @selected_catalog && @list.present? %>
  <strong><%= t(".selected_catalog") %>: "<%= @selected_catalog.name %>"</strong>
  (<%= link_to t(".back_to_list"), favorites_path %>)
  <hr>
<% end %>

<% if @list.blank? %>
  <div class="alert alert-warning">
    <%= empty_searches_warning(@selected_catalog) %>
  </div>
<% end %>

<% @list.unpaginaged_items.each_with_index do |search, i| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= search.name.presence || search.related_search.uuid %>
        <%#= search_list_link(item_list, item, i, item_display_name(item)) %>
      </h4>
      <%#= item_summary(item) %>
    </div>
    <%= render "searches/buttons", :search => search if favorites_scoped? %>
  </div>
<% end %>

<%#= render_item_list(@list) %>

<%#= paginate(@list.items) %>
