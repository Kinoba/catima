<% provide(:title, t('.new')) %>

<%= bootstrap_form_for([:catalog_admin, @item_type]) do |f| %>

  <%= render("common_form_fields", :f => f) %>

  <hr>

  <%= f.primary(t('.create')) %>
  <%= submit_and_add_another(f) %>
  <%= link_to t("cancel"), catalog_admin_setup_path, :class => "btn btn-link" %>

<% end %>
