<% provide(:title, "Import #{item_type.name_plural} from CSV") %>

<%= bootstrap_form_for([:catalog_admin, @csv_import]) do |f| %>
  <%= f.attachment_field(:file, :direct => true) %>
  <%= f.primary("Import") %>
  <%= link_to("Cancel", catalog_admin_items_path, :class => "btn btn-link") %>
<% end %>
